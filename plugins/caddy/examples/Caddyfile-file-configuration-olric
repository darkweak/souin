{
    order cache before rewrite
    cache {
        log_level info
        olric {
            path path/to/olric.yml
        }
        ttl 1000s
        default_cache_control no-store
    }
}

80

@match path /test1*
@match2 path /test2*
@matchdefault path /default

cache @match {
    ttl 30s
}

cache @match2 {
    ttl 50s
    default_cache_control "public, max-age=86400"
}

cache @matchdefault {
    ttl 25s
}

cache * {
}
