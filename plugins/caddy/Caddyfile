{
    order cache before rewrite
    log {
        level debug
    }
    cache {
        headers Content-Type Authorization
        log_level debug
        ttl 1000s
    }
}

:80

@match path /test1*
@match2 path /test2*
@matchdefault path /default

cache @match {
    ttl 30s
    headers Cookie
}

cache @match2 {
    ttl 50s
    headers Authorization
}

cache @matchdefault {
    ttl 25s
}
