{
  "apps": {
    "cache": {
      "headers": [
        "Content-Type",
        "Authorization"
      ],
      "log_level": "debug",
      "ttl": "1000s"
    },
    "http": {
      "servers": {
        "": {
          "listen": [":4443"],
          "routes": [
            {
              "match": [
                {
                  "path": [
                    "*"
                  ]
                }
              ],
              "handle": [
                {
                  "handler": "cache"
                },
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "127.0.0.1:9000"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  },
  "logging": {
    "logs": {
      "": {
        "level": "debug"
      }
    }
  }
}
